{
  "name": "@pixiv/three-vrm-materials-mtoon",
  "version": "0.0.1",
  "description": "MToon (toon material) module for @pixiv/three-vrm",
  "license": "MIT",
  "author": "pixiv",
  "files": [
    "/lib/",
    "/ts*/",
    "/types/",
    "LICENSE"
  ],
  "main": "lib/three-vrm-materials-mtoon.min.js",
  "types": "types/index.d.ts",
  "typesVersions": {
    "<3.9": {
      "*": [
        "ts3.4/*"
      ]
    }
  },
  "scripts": {
    "version": "yarn all",
    "all": "yarn && yarn lint && yarn clean && yarn build && yarn docs",
    "dev": "cross-env NODE_ENV=development SERVE=1 rollup -w -c -o lib/three-vrm-materials-mtoon.js",
    "clean": "rimraf docs/ lib/ ts*/ types/",
    "build": "yarn build-dev && yarn build-prod && yarn build-types",
    "build-dev": "cross-env NODE_ENV=development rollup -c -o lib/three-vrm-materials-mtoon.js",
    "build-prod": "cross-env NODE_ENV=production rollup -c -o lib/three-vrm-materials-mtoon.min.js",
    "build-types": "tsc --declaration --declarationDir ./types --emitDeclarationOnly && downlevel-dts types ts3.4/types",
    "docs": "typedoc --out docs --mode file --excludeNotExported",
    "lint": "eslint \"src/**/*.{ts,tsx}\" && prettier \"src/**/*.{ts,tsx}\" --check",
    "lint-fix": "eslint \"src/**/*.{ts,tsx}\" --fix && prettier \"src/**/*.{ts,tsx}\" --write"
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@pixiv/types-vrm-0.0": "0.0.1",
    "@pixiv/types-vrmc-materials-mtoon-1.0": "0.0.1"
  },
  "devDependencies": {
    "three": "^0.125.2"
  },
  "peerDependencies": {
    "three": "^0.125.2"
  }
}
